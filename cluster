Idents(not_al)
maize_in@meta.data[rownames(maize_in@meta.data) %in% Cells(not_al),]$newclusters <- "PE"
al <- subset(S12D_1, UMAP_1 < 5 & UMAP_2 < 6,idents="4") 
Idents(al)
maize_in@meta.data[rownames(maize_in@meta.data) %in% Cells(al),]$newclusters <- "AL"
################# S12D_1 6区属于CZ
